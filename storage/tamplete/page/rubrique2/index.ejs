<div id="rubrique2">
  <h2 class="r2-title"> <span class="green-conv">2</span> <span class="blue-conv"> Informations générales (1/4)</span></h1>
    <div id="ouvrage-bureau">
      <div id="m-ouvrage">
        <div style="display: block">
          <img
            src="<%= baseURL %>tamplete/img/fleche.png"
            style="width: 10px; height: 10px; float: left; margin-right: 4px;"
          >
          <h3 class="sub-rubric-size">
            <span class="green-conv">2.1</span>
            <span class="blue-conv"> Maître d'ouvrage</span>
          </h3>
          <div style="clear: both; display: block; content: '';"></div>
        </div>
        <div class="lineContainer">
        </div>
        <div style="margin-left: 10px; display: block;">
          <% if(typeof csv == 'object' && csv.maitre_de_ouvrage && csv.maitre_de_ouvrage.nom_all){ %>
          <p><span class="r2-attribut">NON : </span><span class="r2-value"><%= csv.maitre_de_ouvrage.nom_all %></span></p>
          <% }%>
          <% if(typeof csv == 'object' && csv.maitre_de_ouvrage && csv.maitre_de_ouvrage.nom_all){ %>
          <p><span class="r2-attribut">ADRESSE :</span> <br />
          <span class="r2-value"><%= csv.maitre_de_ouvrage.adresse %></span></p>
          <% } %>
          <% if(typeof csv == 'object' && csv.maitre_de_ouvrage && csv.maitre_de_ouvrage.statut){ %>
          <p><span class="r2-attribut">STATUT : </span><span class="r2-value"><%= csv.maitre_de_ouvrage.statut %></span></p>
          <% } %>
        </div>
        <div class="lineContainer"></div>
      </div>
      <div id="bureau" style="padding-left: 10px;">
        <div style="display: block">
          <img
            src="<%= baseURL %>tamplete/img/fleche.png"
            style="width: 10px; height: 10px; float: left; margin-right: 4px;"
          >
          <h3 class="sub-rubric-size">
            <span class="green-conv">2.2</span>
            <span class="blue-conv"> Bureau d'étude</span>
          </h3>
          <div style="clear: both; display: block; content: '';"></div>
        </div>
        <div class="lineContainer">
        </div>
        
        <div class="sub-rubric-p">
          <p><span class="r2-attribut">NON : </span><span class="r2-value"></span></p>
          <p><span class="r2-attribut">CONCTACT : </span><span class="r2-value"></span></p>
          <p><span class="r2-attribut">TÉL : </span><span class="r2-value">00 00 00 00</span></p>
          <p><span class="r2-attribut">E-MAIL : </span><span class="r2-value">estivenetiv@quilleniam.xx</span></p>
        </div>
        <div class="lineContainer">
        </div>
      </div>
    </div>

    <div id="r-dossier" style="padding-left: 10px;">
      <div style="display: block">
        <img
          src="<%= baseURL %>tamplete/img/fleche.png"
          style="width: 10px; height: 10px; float: left; margin-right: 4px;"
        >
        <h3 class="sub-rubric-size">
          <span class="green-conv">2.3</span>
          <span class="blue-conv"> Référence dossier xxxxxxx</span>
        </h3>
        <div style="clear: both; display: block; content: '';"></div>
      </div>
      <div class="lineContainer">
      </div>
      <div class="sub-rubric-p">
        <p><span class="r2-attribut">OBJET : </span><span class="r2-value">Etude Thermique réglementaire selon
          la méthode de calcul Th-C-E-ex conçue par le CSTB
          </p>
        <% if(typeof csv == 'object' && csv.reference_dossier && csv.reference_dossier.adresse){ %>
        <p><span class="r2-attribut">ADRESSE BÂTIMENT : </span><span class="r2-value"><%= csv.reference_dossier.adresse %></span></p>
        <% } %>
        <% if(typeof csv == 'object' && csv.reference_dossier && csv.reference_dossier.adresse){ %>
        <p><span class="r2-attribut">CP/VILLE : </span><span class="r2-value"><%= csv.reference_dossier.codepostal + csv.reference_dossier.ville %></span></p>
        <% }  %>
      </div>
      <div class="lineContainer">
      </div>
    </div>
    <div id="calculs" style="padding-left: 10px;">
      <div style="display: block">
        <img
          src="<%= baseURL %>tamplete/img/fleche.png"
          style="width: 10px; height: 10px; float: left; margin-right: 4px;"
        >
        <h3 class="sub-rubric-size">
          <span class="green-conv">2.4</span>
          <span class="blue-conv"> Calculs</span>
        </h3>
        <div style="clear: both; display: block; content: '';"></div>
      </div>
      <div class="lineContainer">
      </div>
      <div class="sub-rubric-p">
        <% if(typeof xml == 'object' && xml.version_log){ %>
          <p><span class="r2-attribut">LOGICIEL VERSION : </span><span class="r2-value"><%= xml.version_log %></span></p>
          <% } %>
          <% if(typeof xml == 'object' && xml.temp_ext_base_corrige){ %>
          <p><span class="r2-attribut">TEMP. EXT. DE BASE : </span><span class="r2-value"><%= xml.temp_ext_base_corrige %></span></p>
          <% }  %>
      </div>
      <div class="lineContainer">
      </div>
    </div>
    <div style="clear: both;"></div>
    <div id="site" style="padding-left: 10px;">
      <div style="display: block">
        <img
          src="<%= baseURL %>tamplete/img/fleche.png"
          style="width: 10px; height: 10px; float: left; margin-right: 4px;"
        >
        <h3 class="sub-rubric-size">
          <span class="green-conv">2.5</span>
          <span class="blue-conv"> Site audité le xx/xx/xxxx</span>
        </h3>
        <div style="clear: both; display: block; content: '';"></div>
      </div>
      <!-- <h3 class="sub-rubric-size"><span class="green-conv">2.5</span> <span class="blue-conv"> Site audité le xx/xx/xxxx</span></h3> -->
      <img src="<%= baseURL %>tamplete/img/r2/plan.png">
    </div>
    <div id="r2-footer" class="sub-rubric-size r1-footer">
      <div id="r2-footer-l">
        <div style="float: left;">
          <img
            src="<%= baseURL %>tamplete/img/r2/localisation.png"
            style="width: 30px; height: 40px; margin-left: 8px;">
          </div>
        <div style="float: right; display: block; margin-top: 20px;">
          <div style="clear: both;"></div>
          <span class="blue-conv" style="margin-left: -40px;">Dépt.</span>
          <div style="clear: both; height: 5px;"></div>
          <div class="legend-value">
            <% if(typeof xml == 'object' && xml.numero_departement){ %>
              <span style="margin-left: -70px;"><%= xml.numero_departement %></span>
              <% } %>
          </div>
        </div>
      </div>
      <div id="r2-footer-l2">
        <div style="float: left;">
        <img src="<%= baseURL %>tamplete/img/r2/maison.png" style="width: 30px; height: 40px; margin-right: -10px;">
      </div>
        <div style="float: right; display: block; margin-top: 20px; margin-left: 0;">
        <span class="blue-conv" style="text-align: left!important; ">Surface</span>
        <div class="legend-value" style="margin-top: -6px; margin-left: -20px;">
          <% if(typeof csv == 'object' && csv.surface_habitable){ %>
            <p style="font-size: 12px;"></p>
              <span class="blue-conv">Habitable</span>
              <span style="font-size: 10px;"><%= csv.surface_habitable %> m²</span></p>
            <% } else{ %>
            <span > m² </span>
            <% } %>
        </div>
        <div style="clear: both;"></div>
      </div>
      </div>
      <div id="r2-footer-r" style="margin-left: 20px;">
        <div style="float: left;">
        <img src="<%= baseURL %>tamplete/img/r2/calendrier.png" style="width: 30px; height: 40px;">
        </div>
        <div style="float: right; display: block; margin-top: 20px;">
        <div style="clear: both;"></div>
        <span class="blue-conv" style="margin-left: -70px;">Année de</span>
        <div class="legend-value" style="margin-top: -6px; margin-left: 0;">
          <% if(typeof csv == 'object' && csv.annee_de_construction){ %>
            <p style="margin-left: -70px; font-size: 12px;">
              <span class="blue-conv">Construction</span>
              <span style="font-size: 10px;"><%= csv.annee_de_construction %></span></p>
            <% } else{ %>
            <span > m² </span>
            <% } %>
        </div>
      </div>
      </div>
    </div>
    <div style="clear: both;"></div>
    <div id="place-footer"></div>
</div>
